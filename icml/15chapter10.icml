              <PathPointType Anchor="-512.00000 -596.00000" LeftDirection="-512.00000 -596.00000" RightDirection="-512.00000 -596.00000" />
              <PathPointType Anchor="-512.00000 596.00000" LeftDirection="-512.00000 596.00000" RightDirection="-512.00000 596.00000" />
              <PathPointType Anchor="512.00000 596.00000" LeftDirection="512.00000 596.00000" RightDirection="512.00000 596.00000" />
              <PathPointType Anchor="512.00000 -596.00000" LeftDirection="512.00000 -596.00000" RightDirection="512.00000 -596.00000" />
            </PathPointArray>
          </GeometryPathType>
        </PathGeometry>
      </Properties>
      <Image Self="ue6" ItemTransform="1.00000 0 0 1.00000 -512.00000 -596.00000">
