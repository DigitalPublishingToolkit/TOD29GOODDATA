              <PathPointType Anchor="240.00000 339.50000" LeftDirection="240.00000 339.50000" RightDirection="240.00000 339.50000" />
              <PathPointType Anchor="240.00000 -339.50000" LeftDirection="240.00000 -339.50000" RightDirection="240.00000 -339.50000" />
