        <PathGeometry>
          <GeometryPathType PathOpen="false">
            <PathPointArray>
              <PathPointType Anchor="-657.50000 -446.00000" LeftDirection="-657.50000 -446.00000" RightDirection="-657.50000 -446.00000" />
              <PathPointType Anchor="-657.50000 446.00000" LeftDirection="-657.50000 446.00000" RightDirection="-657.50000 446.00000" />
              <PathPointType Anchor="657.50000 446.00000" LeftDirection="657.50000 446.00000" RightDirection="657.50000 446.00000" />
              <PathPointType Anchor="657.50000 -446.00000" LeftDirection="657.50000 -446.00000" RightDirection="657.50000 -446.00000" />
            </PathPointArray>
